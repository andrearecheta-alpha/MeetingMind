function mm {
       Set-Location C:\Users\andre\Projects\MeetingMind
   }

   function mmstart {
       Set-Location C:\Users\andre\Projects\MeetingMind
       $env:PYTHONPATH="src"
       python -m uvicorn meetingmind.main:app --host 0.0.0.0 --port 8000
   }

   function mmtest {
       Set-Location C:\Users\andre\Projects\MeetingMind
       $env:PYTHONPATH="src"
       python -m pytest tests/ -v
   }

   function mmcc {
       Set-Location C:\Users\andre\Projects\MeetingMind
       claude
   }
   